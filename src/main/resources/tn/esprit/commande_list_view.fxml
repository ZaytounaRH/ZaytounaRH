    <stylesheets>
        <URL value="@Style.css"/>
    </stylesheets>

    <VBox layoutX="50.0" layoutY="50.0" spacing="20">
        <TextField fx:id="searchField" promptText="Rechercher par Nom du Fournisseur"/>
        <FlowPane fx:id="flowPaneCommande" prefHeight="400.0" prefWidth="980.0" hgap="20" vgap="20"/>
        <Button layoutX="460.0" layoutY="550.0" onAction="#goBack" text="Retour"/>
    </VBox>
</AnchorPane>
